{
  "version": 2,
  "framework": "vite",
  "installCommand": "npm install",
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist/spa",
  "redirects": [
    {
      "source": "/admin/:path*",
      "destination": "https://admin-three-zeta-82.vercel.app/:path*",
      "permanent": false
    },
    {
      "source": "/admin",
      "destination": "https://admin-three-zeta-82.vercel.app/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/backend/(.*)",
      "destination": "https://velvetwords-backend.vercel.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}